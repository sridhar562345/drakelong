/*
 * @flow
 */

{% if extra_imports|length > 0 %}{{extra_imports}}{%endif%}
{% if import_flag %}import type { IObservableArray } from 'mobx'{%endif%}
import { observable, action } from "mobx"
{% if api_type_import|length > 0 %}import type { {{api_type_import}} } from "./type.js"{%endif%}

class {{model_name}}{
    {% for each_field, type, is_required in fields %}@observable {{each_field}}{%if not is_required%}?{%endif%}:{{type}}{% if not loop.last %}
    {% endif %}{% endfor %}

    constructor({{camel_name}}:API{{model_name}}){
        {% for each_field,right_side in constructor %}this.{{each_field}}={% if 'new ' not in right_side %}{{camel_name}}.{% endif %}{{right_side}}
        {% endfor %}
    }

    {% for name,each_field,type1,default_symbol,new_field,is_required in actions %}@action.bound
    set{{name}}({{each_field}}{%if not is_required%}?{% endif %}{{type1}}){
        this.{{each_field}}={{new_field}}
    }
    {% if not loop.last %}
    {% endif %}{% endfor %}
}
export default {{model_name}}
